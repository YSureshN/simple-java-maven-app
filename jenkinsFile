pipeline {
    agent any
    stages {
        stage ("Build") {
            steps {
                parallel {
                    "Build1": {
                        sh 'echo %Name%'
                    },
                    "Build2": {
                        sh 'echo Build2'
                    }
                }
            }
        }
        stage ('Test') {
            steps {
                sh 'echo Test'
            }
        }
    }
}
