pipeline {
    agent any
    tools {
        maven 'maven-3.6.1' 
    }
    stages {
        stage ("Build") {
            steps {
                parallel {
                    "Build1": {
                        sh 'echo %Name%'
                    },
                    "Build2": {
                        sh 'echo Build2'
                    }
                }
            }
        }
        stage ('Test') {
            steps {
                sh 'echo Test'
            }
        }
    }
 }
